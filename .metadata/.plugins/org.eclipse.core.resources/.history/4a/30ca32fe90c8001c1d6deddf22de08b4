package com.springboot.support.customdetailshow;

import java.util.Date;
import java.util.List;

public class ViewTicketDetailsWithComment {
    private int ticket_id; 	
	private String category_id;
	private String sub_category_id;
	private String assignee_Id;
	private long reported_Id; 
	private String subject,
	description,
    status_id,
	priority_id;
	private Date create_datetime,
	last_modified_datetime;
	private List<TicketComments> ticket_comment;
	
	
	
	
	public ViewTicketDetailsWithComment() {
		super();
	}
	

	public ViewTicketDetailsWithComment(int ticket_id, String category_id, String sub_category_id, String assignee_Id,
			long reported_Id, String subject, String description, String status_id, String priority_id,
			Date create_datetime, Date last_modified_datetime, List<TicketComments> tc) {
		super();
		this.ticket_id = ticket_id;
		this.category_id = category_id;
		this.sub_category_id = sub_category_id;
		this.assignee_Id = assignee_Id;
		this.reported_Id = reported_Id;
		this.subject = subject;
		this.description = description;
		this.status_id = status_id;
		this.priority_id = priority_id;
		this.create_datetime = create_datetime;
		this.last_modified_datetime = last_modified_datetime;
		this.ticket_comment=tc;
	}



	public int getTicket_id() {
		return ticket_id;
	}


	public void setTicket_id(int ticket_id) {
		this.ticket_id = ticket_id;
	}


	public String getCategory_id() {
		return category_id;
	}


	public void setCategory_id(String category_id) {
		this.category_id = category_id;
	}


	public String getSub_category_id() {
		return sub_category_id;
	}


	public void setSub_category_id(String sub_category_id) {
		this.sub_category_id = sub_category_id;
	}


	public String getAssignee_Id() {
		return assignee_Id;
	}


	public void setAssignee_Id(String assignee_Id) {
		this.assignee_Id = assignee_Id;
	}


	public long getReported_Id() {
		return reported_Id;
	}


	public void setReported_Id(long reported_Id) {
		this.reported_Id = reported_Id;
	}


	public String getSubject() {
		return subject;
	}


	public void setSubject(String subject) {
		this.subject = subject;
	}


	public String getDescription() {
		return description;
	}


	public void setDescription(String description) {
		this.description = description;
	}


	public String getStatus_id() {
		return status_id;
	}


	public void setStatus_id(String status_id) {
		this.status_id = status_id;
	}


	public String getPriority_id() {
		return priority_id;
	}


	public void setPriority_id(String priority_id) {
		this.priority_id = priority_id;
	}


	public Date getCreate_datetime() {
		return create_datetime;
	}


	public void setCreate_datetime(Date create_datetime) {
		this.create_datetime = create_datetime;
	}


	public Date getLast_modified_datetime() {
		return last_modified_datetime;
	}


	public void setLast_modified_datetime(Date last_modified_datetime) {
		this.last_modified_datetime = last_modified_datetime;
	}


	public List<TicketComments> getTicket_comment() {
		return ticket_comment;
	}


	public void setTicket_comment(List<TicketComments> ticket_comment) {
		this.ticket_comment = ticket_comment;
	}


	@Override
	public String toString() {
		return "ViewTicketDetailsWithComment [ticket_id=" + ticket_id + ", category_id=" + category_id
				+ ", sub_category_id=" + sub_category_id + ", assignee_Id=" + assignee_Id + ", reported_Id="
				+ reported_Id + ", subject=" + subject + ", description=" + description + ", status_id=" + status_id
				+ ", priority_id=" + priority_id + ", create_datetime=" + create_datetime + ", last_modified_datetime="
				+ last_modified_datetime + ", ticket_comment=" + ticket_comment + "]";
	}




	
}
