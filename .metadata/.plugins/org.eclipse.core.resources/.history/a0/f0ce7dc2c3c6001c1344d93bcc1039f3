INSERT INTO sub_category (sub_category_id ,sub_category_desc) SELECT 999, 'sub category desc' 
WHERE (SELECT COUNT(*) FROM sub_category WHERE sub_category_id=999 AND sub_category_desc='sub category desc') = 0;